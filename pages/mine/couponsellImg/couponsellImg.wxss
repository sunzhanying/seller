/* pages/mine/couponsellImg/couponsellImg.wxss */
@import '../couponSell/couponSell.wxss';

page {
  width: 100%;
  height: 100%;
}
.blackFs{
  color: black !important;
}
.picker {
  width: 100%;
  height: 80rpx;
  background: rgba(242, 242, 242, 1);
  opacity: 1;
  border-radius: 8rpx;
  padding: 20rpx 0 20rpx 30rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  margin-top: 10rpx;
  color: #808080;
}

.couponsellImg {
  width: 100%;
  height: 100%;
}

.txtTitle {
  width: 128rpx;
  height: 44rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 400;
  line-height: 44rpx;
  color: rgba(51, 51, 51, 1);
  opacity: 1;
  margin-top: 40rpx;
  margin-left: 40rpx;
}

.cardBox {
  margin: 10rpx 20rpx;
  background-color: #fff;
  padding: 10rpx 20rpx 0;
  box-sizing: border-box;
}

.default {
  margin-top: 10rpx;
  display: flex;
  padding-bottom: 20rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #e6e6e6;
}

.mgt20 {
  margin-top: 20rpx;
}

.default_left {
  width: 170rpx;
  height: 170rpx;
  background: rgba(242, 242, 242, 1);
  opacity: 1;
  border-radius: 8rpx;
  position: relative;
}

.roundAdd {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.default_inp {
  width: 490rpx;
  height: 80rpx;
  background: rgba(242, 242, 242, 1);
  opacity: 1;
  border-radius: 8rpx;
  padding: 20rpx 30rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  box-sizing: border-box;
}

.short_inp {
  /* width: 430rpx; */
  width: 100%;
  height: 80rpx;
  background: rgba(242, 242, 242, 1);
  opacity: 1;
  border-radius: 8rpx;
  padding: 20rpx 30rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  box-sizing: border-box;
}

.default_right {
  width: 100%;
  margin-left: 10rpx;
}

.mgt10 {
  margin-top: 10rpx;
}

.cardAdd {
  display: flex;
  padding-bottom: 10rpx;
}

.deteleArea {
  width: 50rpx;
  height: 170rpx;
  background: rgba(242, 242, 242, 1);
  opacity: 1;
  border-radius: 8rpx;
  margin-left: 10rpx;
  position: relative;
}

.signupNode {
  margin: 20rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
}

.nodeDetail {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 600;
}
