.head_portrait_img {
  width: 120rpx;
  height: 120rpx;
  margin: 0 14rpx 0 20rpx;
  border-radius: 50%;
  margin-top: 34rpx;

}

.mine_banner_img {
  position: relative;
  width: 100%;
  display: block;
  z-index: 1;
}

.mine_nav_icon {
  width: 52rpx;
  height: 36rpx;
  margin-bottom: 16rpx;
}

.mine_nav_box {
  width: 100%;
  height: 260rpx;
  background: #fff;
  margin: 20rpx auto 20rpx;
  box-shadow: 0px 0px 12rpx 0px rgba(0, 0, 0, 0.2);
  border-radius: 0 0 8rpx 8rpx;
}

.mine_li_radius {
  margin-top: 30rpx;
}

.mine_title_box {
  height: 84rpx;
  border-bottom: 2rpx solid #ccc;
  margin-bottom: 34rpx;
  margin: 0 20rpx;
}

.mine_nav_ul {
  margin: 0 34rpx;
  text-align: center;
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  height: 176rpx;
}

.arrow_icon {
  width: 12rpx;
  height: 20rpx;
  margin-left: 12rpx;
}

.mine_list_box {
  width: 710rpx;
  background: #fff;
  box-shadow: 0px 0px 12rpx 0px rgba(0, 0, 0, 0.2);
}

.mine_list {
  width: 655rpx;
  font-size: 28rpx;
  font-weight: 400;
  margin: 0 20rpx;
  color: rgba(51, 51, 51, 1);
}

.border_bottom {
  border-bottom: 2rpx solid #ccc;
}

/* .mine_list:not(:last-of-type) {
    border-bottom: 2rpx solid #ccc;
} */

.mine_pad_bot {
  padding-bottom: 14rpx !important;
}

.mine_pic_text {
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(231, 199, 120, 1);
  line-height: 40rpx;
  padding-bottom: 10rpx;
}

.mine_pay_card {
  width: 180rpx;
}

.mine_right_arrow1, .mine_right_arrow2 {
  width: 0;
  height: 0;
  display: block;
  position: absolute;
  right: 0;
  top: 15rpx;
  border-top: 10rpx transparent dashed;
  border-right: 10rpx transparent dashed;
  border-bottom: 10rpx transparent dashed;
  border-left: 10rpx #333 solid;
  overflow: hidden;
}

.mine_right_arrow1 {
  right: -4rpx; /*重要*/
  border-left: 10rpx rgba(231, 199, 120, 1) solid;
}

.mine_right_arrow2 {
  border-left: 10rpx #333 solid;
}

.mine_bemerber_btn {
  margin: 0;
  padding: 0;
  background: #333;
  height: 46rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(231, 199, 120, 1);
  line-height: 46rpx;
  box-sizing: border-box;
}

.mine_nav_top {
  width: 100%;
  height: 138rpx;
