.contact_service_img {
  width: 36rpx;
  height: 36rpx;
  margin-right: 18rpx;
  vertical-align: middle;
}

.mine_list_li {
  width: 616rpx;
  height: 90rpx;
}
.btn_extract{
  flex: 1;
}
.head_portrait_info {
  width: 120rpx;
  height: 120rpx;
  background: #e7c778;
  margin: 0;
  padding: 0;
  left: -150rpx;
  opacity: 0;
}

.mine_bg {
  width: 100%;
  height: 260rpx;
}

.mine_title_order {
  padding-left: 8rpx;
  border-left: 4rpx solid #e7c778;
}

.mine_scroll_box {
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
}

.mine_card_box {
  background: #333;
  height: 100rpx;
  color: rgba(231, 199, 120, 1);
  padding: 0 20rpx;
  font-size: 32rpx;
  border-radius: 8rpx 8rpx 0 0;
}

.mine_banner {
  position: relative;
  display: flex;
  align-items: center;
  flex-direction: column;
}

.mine_banner .mine_user_info {
  position: absolute;
  top: 0;
  z-index: 2;
  width: 710rpx;
  /* height: 154rpx; *//* background: rgba(255, 255, 255, 1); *//* box-shadow: 0px 0px 12rpx 0px rgba(0, 0, 0, .2); *//* border-radius: 8rpx; */
